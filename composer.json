{
    "name": "super-kernel/attribute",
    "description": "The attribute component for super-kernel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SuperKernel\\Attribute\\": "src/"
        }
    },
    "autoload-dev": {
        "SuperKernelTest\\Attribute\\": "tests/src/"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.4",
        "ext-pcntl": "*",
        "nikic/php-parser": "^5.7"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../di"
        }
    ],
    "require-dev": {
        "super-kernel/di": "dev-main"
    }
}
